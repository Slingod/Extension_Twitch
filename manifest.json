{
  "manifest_version": 3,
  "name": "Extension Live Du Slingo",
  "version": "0.2.0",
  "description": "Notifie quand un streamer passe en live.",
  "homepage_url": "https://TON_SITE/privacy",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "Live Notifier",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "alarms", "notifications"],
  "host_permissions": ["https://late-lab-301c.slingo-drisca.workers.dev/*"]
}
